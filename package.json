{
	"name": "guapi",
	"displayName": "Guapi",
	"description": "一个自动化部署作业的实验平台",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "Guapi",
					"title": "Guapi",
					"icon": "media/整理.svg"
				}
			]
		},
		"views": {
			"Guapi": [
				{
					"id": "student",
					"name": "学生面板"
				},
				{
					"id": "class",
					"name": "课程"
				},
				{
					"id": "exam",
					"name": "实验"
				}
			]
		},
		"commands": [
			{
				"command": "student.login",
				"title": "登陆"
			},
			{
				"command": "student.register",
				"title": "注册"
			},
			{
				"command": "student.logout",
				"title": "退出"
			},
			{
				"command": "class.join",
				"title": "加入班级"
			},
			{
				"command": "class.flush",
				"title": "刷新"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "student.login",
					"when": "view == student"
				},
				{
					"command": "student.register",
					"when": "view == student"
				},
				{
					"command": "student.logout",
					"when": "view == student"
				},
				{
					"command": "class.join",
					"when": "view == class"
				},
				{
					"command": "class.flush",
					"when": "view == class"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@types/request": "^2.48.4"
	}
}
